	<!-- 
		테이블 태그
		<table>
		<tr> : table row	: 행 추가
		<th> : table head	: 
		<td> : table date	: 테두리 선 굵기
			[속성]
			1. border = "1" : 테두리 선 굵기
			2. rowspan = 행 갯수 		: 행 병합
			3. clospan = 열 갯수		: 열 병합
	 -->

	<table border = "1"> <!-- table start -->
		<tr>	<!-- 행 추가 -->
			<th> 지역 </th>
			<th> 최저기온 </th>
			<th> 최고기온 </th>
		</tr>
		<tr>
			<td>서울</td> <td>20</td> <td>30</td>
			<tr> <td>대구</td> <td>10</td> <td>30</td>
			<tr> <td>광주 	</td> <td>50</td> <td>40</td>
		</tr>
	</table> <!-- table end -->
	
		<table border = "1" style="border-collapse: collapse; ">
			<tr> <th>출발</th> <th>도착</th><th clospan = "2">객실/요금</th> <th>예약</th>
			</tr>
			<tr>
				<td rowspan = "3"> 서울 </td>
				<td> 부산 </td>
				<td> 일반실 </td>
				<td> 50,000원 </td>
				<td> <input type="button" value="예약신청">
			</tr>
			<tr>
